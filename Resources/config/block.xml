<?xml version="1.0" encoding="UTF-8"?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sonatra_block.type_extension.block" class="Sonatra\Bundle\BootstrapBundle\Block\Extension\BlockExtension">
            <tag name="sonatra_block.type_extension" alias="block" />
        </service>

        <service id="sonatra_block.type_extension.collection" class="Sonatra\Bundle\BootstrapBundle\Block\Extension\CollectionExtension">
            <tag name="sonatra_block.type_extension" alias="collection" />
        </service>

        <service id="sonatra_block.type_extension.repeated" class="Sonatra\Bundle\BootstrapBundle\Block\Extension\RepeatedExtension">
            <tag name="sonatra_block.type_extension" alias="repeated" />
        </service>

        <service id="sonatra_block.type.fieldset" class="Sonatra\Bundle\BootstrapBundle\Block\Type\FieldsetType">
            <tag name="sonatra_block.type" alias="fieldset" />
        </service>

        <service id="sonatra_block.type.image" class="Sonatra\Bundle\BootstrapBundle\Block\Type\ImageType">
            <tag name="sonatra_block.type" alias="image" />
        </service>

        <service id="sonatra_block.type.glyphicon" class="Sonatra\Bundle\BootstrapBundle\Block\Type\GlyphiconType">
            <tag name="sonatra_block.type" alias="glyphicon" />
        </service>

        <service id="sonatra_block.type.button_group" class="Sonatra\Bundle\BootstrapBundle\Block\Type\ButtonGroupType">
            <tag name="sonatra_block.type" alias="button_group" />
        </service>

        <service id="sonatra_block.type.button_toolbar" class="Sonatra\Bundle\BootstrapBundle\Block\Type\ButtonToolbarType">
            <tag name="sonatra_block.type" alias="button_toolbar" />
        </service>

        <service id="sonatra_block.type.well" class="Sonatra\Bundle\BootstrapBundle\Block\Type\WellType">
            <tag name="sonatra_block.type" alias="well" />
        </service>

        <service id="sonatra_block.type.button" class="Sonatra\Bundle\BootstrapBundle\Block\Type\ButtonType">
            <tag name="sonatra_block.type" alias="button" />
        </service>

        <service id="sonatra_block.type.dropdown" class="Sonatra\Bundle\BootstrapBundle\Block\Type\DropdownType">
            <tag name="sonatra_block.type" alias="dropdown" />
        </service>

        <service id="sonatra_block.type.dropdown_item" class="Sonatra\Bundle\BootstrapBundle\Block\Type\DropdownItemType">
            <tag name="sonatra_block.type" alias="dropdown_item" />
        </service>

        <service id="sonatra_block.type.divider" class="Sonatra\Bundle\BootstrapBundle\Block\Type\DropdownDividerType">
            <tag name="sonatra_block.type" alias="dropdown_divider" />
        </service>

        <service id="sonatra_block.type.header" class="Sonatra\Bundle\BootstrapBundle\Block\Type\DropdownHeaderType">
            <tag name="sonatra_block.type" alias="dropdown_header" />
        </service>

        <service id="sonatra_block.type.nav" class="Sonatra\Bundle\BootstrapBundle\Block\Type\NavType">
            <tag name="sonatra_block.type" alias="nav" />
        </service>

        <service id="sonatra_block.type.nav_item" class="Sonatra\Bundle\BootstrapBundle\Block\Type\NavItemType">
            <tag name="sonatra_block.type" alias="nav_item" />
        </service>

        <service id="sonatra_block.type.tab_content" class="Sonatra\Bundle\BootstrapBundle\Block\Type\TabContentType">
            <tag name="sonatra_block.type" alias="tab_content" />
        </service>

        <service id="sonatra_block.type.tab_pane" class="Sonatra\Bundle\BootstrapBundle\Block\Type\TabPaneType">
            <tag name="sonatra_block.type" alias="tab_pane" />
        </service>
    </services>
</container>
