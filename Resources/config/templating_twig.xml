<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="sonatra_bootstrap.assetic.twig_extension.class">Sonatra\Bundle\BootstrapBundle\Twig\Extension\AsseticExtension</parameter>
    </parameters>

    <services>
        <service id="sonatra_bootstrap.assetic.twig_extension" class="%sonatra_bootstrap.assetic.twig_extension.class%" public="false">
            <tag name="twig.extension" />
            <property name="container" type="service" id="service_container" />
        </service>
    </services>
</container>
