            {{ block_widget(child) }}
        {% endfor %}
    </div>
{% endspaceless %}
{% endblock row_widget %}

{% block col_widget %}
{% spaceless %}
    {% set col_class = '' %}

    {% for item in grid %}
        {% set col_class = col_class ~ item ~ ' ' %}
    {% endfor %}

    {% for item in offset %}
        {% set col_class = col_class ~ item ~ ' ' %}
    {% endfor %}

    {% set attr = attr|merge({'class': (col_class ~ ' ' ~ attr.class|default(''))|trim}) %}

    <div{{ block('widget_container_attributes') }}>
        {{- value|raw -}}
        {% for child in block %}
            {{ block_widget(child) }}
        {% endfor %}
    </div>
{% endspaceless %}
{% endblock col_widget %}

{% block clearfix_widget %}
{% spaceless %}
