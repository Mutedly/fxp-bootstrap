    {% if size is not null %}
        {% set attr = attr|merge({'class': (attr.class|default('') ~ ' btn-group-' ~ size)|trim}) %}
    {% endif %}

    {% if justified %}
        {% set attr = attr|merge({'class': (attr.class|default('') ~ ' btn-group-justified')|trim}) %}
    {% endif %}
    {{- value|raw -}}
    {{ block('block_widget_compound') }}
{% endspaceless %}
{% endblock button_group_widget %}


