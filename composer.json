{
    "name" : "sonatra/bootstrap",
    "type" : "library",
    "description" : "Twig DSL for construct a Twitter Bootstrap interface",
    "keywords" : ["twitter bootstrap 3", "block", "dsl", "template"],
    "homepage" : "https://github.com/sonatra/sonatra-bootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "php" : ">=7.0.0",
        "sonatra/block" : "^1.0.0"
    },
    "require-dev" : {
        "doctrine/orm": "^2.5.0",
        "gedmo/doctrine-extensions": "^2.4.0"
    },
    "suggest": {
        "bower-asset/html5shiv": "^3.7.0",
        "bower-asset/respond": "^1.4.0",
        "bower-asset/bootstrap": "^3.3.0",
        "fxp/composer-asset-plugin": "For manage NPM/Bower dependencies with Composer"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Component\\Bootstrap\\" : "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
