{
    "name" : "fxp/bootstrap",
    "type" : "library",
    "description" : "Twig DSL for construct a Twitter Bootstrap interface",
    "keywords" : ["twitter bootstrap 3", "block", "dsl", "template"],
    "homepage" : "https://github.com/fxpio/fxp-bootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "php" : "^7.1.0",
        "fxp/block" : "^1.1.0"
    },
    "require-dev" : {
        "fxp/doctrine-extensions" : "^1.1.0",
        "doctrine/orm": "^2.6.0",
        "gedmo/doctrine-extensions": "^2.4.0",
        "foxy/foxy": "^1.0.0"
    },
    "config": {
        "foxy": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": { "Fxp\\Component\\Bootstrap\\" : "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.1-dev"
        }
    }
}
